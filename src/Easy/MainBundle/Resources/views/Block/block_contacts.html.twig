<div class="col-md-11" style="margin-bottom: 50px;">
    <div class="stick"></div>
    <p class='title'>{{ content.getName()}}</p>
    <div class="row">
        <div class="col-md-4">
            <div class="advantage address purple red" style="text-align: right;">
                {% for contact in content.contacts %}
                    <a href="javascript:;" address="{{ loop.index }}" data-coordinates="[{{ contact.coordinates }}]">{{ contact.name }}</a>
                {% endfor %}
                {% if content.id is not null %}
                    <div class="buttonSquare" style="clear: both;float: left;">
                        <a href="{{ path('easy_main_contacts', {'id': content.id }) }}">Узнать подробнее</a>
                    </div>
                {% else %}
                    <div class="buttonSquare" style="clear: both;float: left;">
                        <a href="{{ path('easy_main_contacts', {'id': 'all_show' }) }}">Узнать подробнее</a>
                    </div>
                {% endif %}
            </div>
        </div>
        <div class="col-md-8">
            <div id="map" style="width: 100%; height: 320px; position: relative;"></div>
        </div>
    </div>
</div>