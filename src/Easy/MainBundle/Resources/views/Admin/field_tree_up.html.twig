{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}

    {% spaceless %}
        {% if object.parent != null %}
            {% if object.parent.children[0].id != object.id %}
                <a href="{{ path('page_tree_up', {'page_id': object.id}) }}">
                    <i class="fa fa-arrow-up"></i>
                </a>
            {% endif %}
        {% endif %}
    {% endspaceless %}
{% endblock %}